# FamilyUp Environment Variables
# IMPORTANT: Copy this file to .env and replace with your actual values

# Mapbox Access Token (required for production)
# Get yours at https://account.mapbox.com/access-tokens/
#
# FOR WEB DEVELOPMENT:
# - Any public token will work
#
# FOR iOS DEPLOYMENT (REQUIRED):
# - Scope: downloads:read (MUST be checked in dashboard)
# - URL restriction: capacitor://localhost (MUST be added)
# - Follow complete guide: docs/MAPBOX_IOS_SETUP.md
#
VITE_MAPBOX_TOKEN=your_mapbox_token_here

# Development Settings
VITE_DEV_MODE=true
VITE_ENABLE_ANALYTICS=false

# Privacy Settings (DO NOT CHANGE - Legal Requirement)
VITE_COLLECT_PII=false
VITE_TRACK_LOCATION=false
VITE_AGGREGATE_DATA_ONLY=true
