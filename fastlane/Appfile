# FamilyUp Fastlane App Configuration

# Apple ID for App Store Connect
app_identifier("com.familyup.app")
apple_id(ENV["APPLE_ID"])
team_id(ENV["TEAM_ID"])

# App Store Connect Provider (if multiple teams)
itc_team_id(ENV["ITC_PROVIDER"])

